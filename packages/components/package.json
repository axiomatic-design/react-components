{
  "name": "@axiomatic/react-components",
  "version": "0.5.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "prebuild": "yarn clean",
    "copy-images": "yarn copy-images:esm && yarn copy-images:cjs",
    "copy-images:esm": "copyfiles -f src/Icon/feather-sprite.svg esm/Icon",
    "copy-images:cjs": "copyfiles -f src/Icon/feather-sprite.svg cjs/Icon",
    "build": "ts-build ./src --cjs --esm && yarn copy-images",
    "test": "echo \"yum\"",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "license": "MIT",
  "repository": "git@github.com:axiomatic-design/react-components.git",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@styled-system/should-forward-prop": "^5.1.5",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "color": "^3.1.3",
    "emotion-reset": "^3.0.0",
    "feather-icons": "^4.28.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@styled-system/css": "^5.1.5",
    "@types/color": "^3.0.1",
    "@types/feather-icons": "^4.7.0",
    "@types/react-helmet": "^6.1.0",
    "@types/reflexbox": "^4.0.2",
    "@types/styled-system__css": "^5.0.14",
    "copyfiles": "^2.4.1"
  },
  "gitHead": "898a785728153be2380f67ec9fb0a67bca0500fb"
}
